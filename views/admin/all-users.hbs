<div class="container mt-5">
  <h2 class="text-center">All Users</h2>
  <div class="table-responsive">
    <table class="table table-striped table-bordered mt-4">
      <thead class="table-dark">
        <tr>
          <th>#</th>
          <th>Profile Image</th>
          <th>Name</th>
          <th>Email</th>
          <th>Phone Number</th>
          <th>Address</th>
          <th>Date Of Birth</th>
          <th>Place</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
        <tr>
          <td>{{@index}}</td>
          <td>
            <img src="{{this.imagePath}}" alt="Profile Image" width="50" height="50" class="rounded-circle">
          </td>
          <td>{{this.Name}}</td>
          <td>{{this.Email}}</td>
          <td>{{this.PhoneNumber}}</td>
          <td>{{this.Address}}</td>
          <td>{{this.DateOfBirth}}</td>
          <td>{{this.Place}}</td>
          
        </tr>
        {{else}}
        <tr>
          <td colspan="8" class="text-center">No users found</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
